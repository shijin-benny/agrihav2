                            min-width: 82px !important;
                            min-height: 24px !important;
                            left: 234px;
                            top: 801px;
                            border: 1px solid #000000;
                            border-radius: 5px;
                            font-family: 'Roboto';
                            font-style: normal;
                            font-weight: 400;
                            font-size: 10px;

                          margin-top: 25px;
                          padding: 10px;
                          border-top: 1px solid #eaeaea;
                          font-family: 'Roboto';
                          font-style: normal;
                          font-weight: 300;
                          font-size: 10px;
                          line-height: 136.19%;
                          /* identical to box height, or 14px */

                          color: #a3a3a3;

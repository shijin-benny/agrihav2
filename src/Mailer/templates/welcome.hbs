                      <div
                        style='
                          text-align: center;
                          margin-top: 20px;
                          padding-top: 10px;
                          padding-bottom: 10px;
                          border-top: 1px solid #eeeeee;
                        '
                      >
                        <img src='cid:image' width='600' height='150' alt='' />
